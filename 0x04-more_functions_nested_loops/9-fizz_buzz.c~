#include <stdio.h>
/**
 *main - entry point
 *
 *Return:1- success
 *
 *Description:prints 1-100 except mult. of 3 and 5
 *
 */
int main(void)
{
	int i;
	char fizz[] = "Fizz";
	char buzz[] = "Buzz";
	char fizBuzz[] = "FizzBuzz";

	for (i = 1; i <= 100; i++)
	{
		if ((i % 3) == 0 && (i % 5) == 0)
		{
			printf("FizzBuzz");
		}
		else if ((i % 3) == 0)
		{
			printf("Fizz");
		}
		else if ((i % 5) == 0)
		{
			printf("Buzz");
		}
		else
		{
			printf("%d", i);
		}
	}
	return (0);
}
